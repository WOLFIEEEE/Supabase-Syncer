# =========================================
# Docker Compose Override for Local Development
# =========================================
# 
# This file enables port mapping for local development.
# Coolify does NOT need this - it uses Traefik reverse proxy.
#
# Usage:
#   1. Copy this file: cp docker-compose.override.yml.example docker-compose.override.yml
#   2. Run: docker-compose up -d
#   3. Access app at: http://localhost:3000
# =========================================

version: '3.8'

services:
  app:
    ports:
      - "3000:3000"  # Expose port for local access

